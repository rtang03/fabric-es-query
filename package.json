{
  "name": "fabric-es-query",
  "version": "0.1.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "queryhandler serverice for cqrs",
  "license": "BSD-3-Clause-Clear",
  "author": "Ross Tang",
  "homepage": "https://github.com/rtang03/fabric-es-query",
  "bugs": {
    "url": "https://github.com/rtang03/fabric-es-query/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "lint": "eslint --max-warnings 10 'src/**/*.{js,ts}'",
    "test:fabricgwateway": "jest fabricGateway.test",
    "test:fabricgwateway-debug": "DEBUG=fabric:gateway jest fabricGateway.test",
    "test:fabricgwateway2": "jest --forceExit --detectOpenHandles fabricGateway.test",
    "server": "node --require './dist/tracing.js' ./dist/app.js",
    "client": "node --experimental-modules ./dist/client.js",
    "start": "node ./dist/sync.js"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/api-metrics": "^0.27.0",
    "@opentelemetry/auto-instrumentations-node": "^0.27.0",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/exporter-prometheus": "^0.27.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.27.0",
    "@opentelemetry/instrumentation-fetch": "^0.27.0",
    "@opentelemetry/instrumentation-winston": "^0.27.0",
    "@opentelemetry/propagator-jaeger": "^1.0.1",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fabric-ca-client": "^2.2.11",
    "fabric-contract-api": "^2.2.2",
    "fabric-network": "^2.2.10",
    "fabric-protos": "^2.2.11",
    "isomorphic-unfetch": "^3.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "winston": "3.3.3",
    "winston2": "npm:winston@2.4.5"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.16.1",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typedoc": "^0.22.10",
    "typedoc-neo-theme": "^1.1.1",
    "typescript": "^4.5.4"
  },
  "prettier": {
    "eslintIntegration": true,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "requirePragma": false,
    "insertPragma": false,
    "printWidth": 100
  }
